{
  "name": "frontend",
  "version": "0.0.13",
  "license": "MIT",
  "scripts": {
    "build": "ng build",
    "dist": "ng build --prod --env=prod --aot=false",
    "dist-trees": "ng build --prod --env=trees --aot=false",
    "dist-staging": "ng build --prod --env=staging --aot=false",
    "dist-prod": "ng build --prod --env=prod --aot=false",
    "docs": "yarn run typedoc -- --out ./src/assets/typedoc --exclude '**/*.spec.ts' ./src/ --module commonjs",
    "e2e:ci": "ng e2e --port=4200 -prod -e dev --watch=false --progress=false",
    "e2e": "ng e2e -prod -e dev --port=4200",
    "lint": "ng lint",
    "ng": "ng",
    "pa11y": "./node_modules/pa11y-ci/bin/pa11y-ci.js",
    "snyk-test": "snyk test",
    "start": "ng serve",
    "test:ci": "ng test --single-run=true --watch=false --progress=false --code-coverage",
    "test": "ng test -sm=false",
    "typedoc": "typedoc",
    "update-version": "npm version patch && node ./replace.build.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.4.4",
    "@angular/compiler": "^4.4.4",
    "@angular/core": "^4.4.4",
    "@angular/forms": "^4.4.4",
    "@angular/http": "^4.4.4",
    "@angular/platform-browser": "^4.4.4",
    "@angular/platform-browser-dynamic": "^4.4.4",
    "@angular/router": "^4.4.4",
    "@cmsgov/design-system-core": "^1.2.0",
    "@types/jasminewd2": "^2.0.3",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "intersection-observer": "^0.4.0",
    "intl": "^1.2.5",
    "ng-in-viewport": "^1.2.0",
    "ng2-auto-complete": "^0.12.0",
    "ng2-file-upload": "^1.2.1",
    "replace-in-file": "^2.5.2",
    "rxjs": "^5.4.1",
    "web-animations-js": "^2.2.5",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "^1.1.3",
    "@angular/compiler-cli": "^4.4.4",
    "@types/jasmine": "^2.5.38",
    "@types/node": "^8.0.14",
    "codelyzer": "^3.1.1",
    "emoji-regex": "^6.5.0",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^4.1.1",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "moment": "^2.18.1",
    "pa11y-ci": "^1.1.1",
    "protractor": "^5.1.2",
    "protractor-jasmine2-screenshot-reporter": "^0.5.0",
    "sinon": "^3.2.1",
    "snyk": "1.42.4",
    "ts-node": "^3.1.0",
    "tslint": "^5.4.3",
    "typescript": "2.3.4",
    "uswds": "^1.2.1"
  }
